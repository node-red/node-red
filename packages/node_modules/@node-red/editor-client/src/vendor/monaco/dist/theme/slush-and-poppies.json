    "editor.background": "#F1F1F1",
    "editor.selectionBackground": "#B0B0FF",
    "editor.lineHighlightBackground": "#00000026",
    "editorCursor.foreground": "#000000",
    "editorWhitespace.foreground": "#BFBFBF"
  }
